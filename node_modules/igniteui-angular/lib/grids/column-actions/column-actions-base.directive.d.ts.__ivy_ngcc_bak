import { IgxColumnComponent } from '../columns/column.component';
export declare abstract class IgxColumnActionsBaseDirective {
    /**
     * @hidden @internal
     */
    actionEnabledColumnsFilter: (value: IgxColumnComponent, index: number, array: IgxColumnComponent[]) => boolean;
    /**
     * @hidden @internal
     */
    abstract get checkAllLabel(): string;
    /**
     * @hidden @internal
     */
    abstract get uncheckAllLabel(): string;
    /**
     * @hidden @internal
     */
    abstract columnChecked(column: IgxColumnComponent): boolean;
    /**
     * @hidden @internal
     */
    abstract toggleColumn(column: IgxColumnComponent): any;
    /**
     * @hidden @internal
     */
    abstract uncheckAll(): any;
    /**
     * @hidden @internal
     */
    abstract checkAll(): any;
}
