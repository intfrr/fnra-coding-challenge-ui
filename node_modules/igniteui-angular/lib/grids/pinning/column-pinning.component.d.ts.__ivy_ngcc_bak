import { ChangeDetectorRef } from '@angular/core';
import { ColumnChooserBaseDirective } from '../column-chooser-base';
import { IgxColumnPinningItemDirective } from './pinning.directive';
export declare class IgxColumnPinningComponent extends ColumnChooserBaseDirective {
    cdr: ChangeDetectorRef;
    constructor(cdr: ChangeDetectorRef);
    /**
     * @hidden
     */
    createColumnItem(container: any, column: any): IgxColumnPinningItemDirective;
    /**
     * @hidden
     */
    checkboxValueChange(event: any, columnItem: IgxColumnPinningItemDirective): boolean;
    /**
     * @hidden @internal
     */
    get pinnableColumns(): any[];
}
